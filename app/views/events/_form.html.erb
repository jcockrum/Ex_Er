<%= form_for @event do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :start_at %><br />
    <%= f.datetime_select :start_at %>
  </p>
  <p>
    <%= f.label :end_at %><br />
    <%= f.datetime_select :end_at %>
  </p>
	<%= f.label "Exercise" %><br />
	<%= f.collection_select(:exercise_id, Exercise.all, :id, :name)%>
  <p><%= f.submit %></p>
<% end %>
